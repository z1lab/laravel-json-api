{
  "name": "z1lab/laravel-json-api",
  "type": "library",
  "description": "A minimal library for normalize Laravel app to return Json Api results.",
  "license": "MIT",
  "authors": [
    {
      "name": "Olimar Ferraz",
      "email": "webmaster@z1lab.com.br"
    }
  ],
  "homepage": "https://github.com/z1lab/laravel-json-api",
  "support": {
    "issues": "https://github.com/z1lab/laravel-json-api/issues",
    "source": "https://github.com/z1lab/laravel-json-api"
  },
  "keywords": [
    "Laravel",
    "Json",
    "Resource",
    "Api"
  ],
  "require": {
    "php": ">=7.1.3",
    "juampi92/api-resources": "1.1.*",
    "optimus/heimdal": "~1.0",
    "illuminate/support": "5.7.*",
    "illuminate/routing": "5.7.*",
    "illuminate/http": "5.7.*",
    "illuminate/validation": "5.7.*",
    "barryvdh/laravel-httpcache": "^0.3.4",
    "barryvdh/laravel-cors": "^0.11.2",
    "illuminate/contracts": "5.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Z1lab\\JsonApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Z1lab\\JsonApi\\ApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
